# This is Main Entry
fn main() -> int {
    !(echo -n Count :);
    !(tput sc);

    let count = 0;
#   let tmp = 2 + 3  * 4 - 2;

    loop {
        if count < 10 {
            count++;
            !(sleep 1);
            !(tput rc);
            !(tput ed);
            !(echo -n $count);
        }
        else {
            return 0;
        }
    }
}
