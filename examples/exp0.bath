# This is Main Entry
fn main() -> int {
    !(echo -n Count :);
    !(tput sc);

    let count: int;

    count = 4;

    #let tmp = 2 + 3 * 4 - 2;
    #!(echo tmp: $tmp);

    loop {
        if count < 10 {
            count++;
            !(sleep 1);
            !(tput rc);
            !(tput ed);
            !(echo -n $count);
        }
        else {
            return 0;
        }
    }
}
